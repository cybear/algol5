{
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^10.12.18",
    "@types/prettier": "^1.16.1",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "jest": "^24.4.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "exportSuites": "ts-node ./commands/exportSuites",
    "compile": "npx ts-node ./commands/compile $*",
    "clean": "npx ts-node ./commands/clean",
    "collect": "ts-node ./collect.ts",
    "makemeta": "ts-node ./makemeta.ts",
    "build": "npm run clean && npm run compile && npm run collect && npm run makemeta",
    "test": "echo 'Running all tests generated by \"npm run generate(Suite|Game)Tests\"\n' && jest",
    "generateSuiteTests": "ts-node ./test/generateSuiteTests",
    "generateGameTests": "ts-node ./test/generateGameTests",
    "testOLD": "ts-node node_modules/tape/bin/tape test/*.spec.ts | node_modules/.bin/tap-spec"
  },
  "dependencies": {}
}
