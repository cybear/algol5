{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "jsx": "react",
    "types": ["node", "jest", "fs-extra", "prettier", "lodash.isequal"]
  },
  "include": ["modules"],
  "exclude": ["archive", "dev"]
}
